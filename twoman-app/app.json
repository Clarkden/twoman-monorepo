{
  "expo": {
    "name": "2 Man",
    "slug": "twoman-app",
    "version": "1.0.4",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "twoman",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1A1A1A"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": false,
      "usesAppleSignIn": true,
      "bundleIdentifier": "dating.twoman.twoman",
      "infoPlist": {
        "NSLocationAlwaysUsageDescription": "Allow 2 Man to use your location to find show nearby profiles and let your profile be discovered by other users",
        "NSLocationWhenInUseUsageDescription": "Allow 2 Man to use your location to find show nearby profiles and let your profile be discovered by other users",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#1A1A1A"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.READ_CONTACTS",
        "android.permission.WRITE_CONTACTS",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "package": "dating.twoman.twoman"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-splash-screen",
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow 2 Man to access your photos to let you share them on your profile."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow 2 Man to use your location to find show nearby profiles and let your profile be discovered by other users."
        }
      ],
      [
        "expo-font",
        {
          "fonts": [""]
        }
      ],
      "expo-apple-authentication",
      "expo-secure-store",
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 24
          },
          "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ],
      ["react-native-appsflyer", {}],
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "2-man-app",
          "organization": "2-man-limited"
        }
      ],
      [
        "expo-contacts",
        {
          "contactsPermission": "Allow 2 Man to access your contacts so you can add your friends."
        }
      ]
    ],
    "experiments": {},
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2b8a60ce-2841-4dba-970b-d148666af7b1"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/2b8a60ce-2841-4dba-970b-d148666af7b1"
    }
  }
}
