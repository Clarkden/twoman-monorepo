{
  "name": "twoman-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "npx expo lint",
    "format": "prettier --write .",
    "prebuild": "expo prebuild",
    "build:ios:dev:local": "eas build --platform ios --profile development --local",
    "build:ios:prod:eas": "eas build --platform ios --profile production",
    "build:ios:prod:eas:submit": "eas build --platform ios --profile production --submit",
    "build:android:dev:local": "eas build --platform android --profile development --local",
    "build:android:prod:eas": "eas build --platform android --profile production",
    "build:android:prod:eas:submit": "eas build --platform android --profile production --submit",
    "generate-types": "node scripts/generate-types.js",
    "sync-schemas": "cp ../twoman-api/websocket-schemas/*.json ./schemas/ && npm run generate-types"
  },
  "jest": {
    "preset": "jest-expo"
  },
